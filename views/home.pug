doctype html
html
    head
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table
        a(href="/update-cobj") Add/Update this table
        table
            tr
                th Name
                th Gender
                th Date of Birth
            each cat in data
                tr
                   td #{cat.properties.name}
                   td #{cat.properties.gender}
                   td #{cat.properties.date_of_birth}